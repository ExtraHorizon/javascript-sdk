{
  "name": "@extrahorizon/javascript-sdk",
  "version": "3.0.2",
  "description": "",
  "main": "build/index.cjs",
  "types": "build/types/index.d.ts",
  "module": "build/index.mjs",
  "author": "Qompium",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "clean": "rimraf build",
    "start": "yarn test",
    "start:watch": "yarn test --watch",
    "build": "yarn clean && yarn build:library && yarn build:declarations",
    "build:library": "rollup --config rollup.config.js",
    "build:declarations": "tsc --project tsconfig.declarations.json",
    "watch": "rollup --config rollup.config.js -w",
    "watch:declarations": "tsc -w --project tsconfig.declarations.json",
    "add-suffix": "node ./node_modules/npm-version-suffix/run-add-suffix.js",
    "remove-suffix": "node ./node_modules/npm-version-suffix/run-remove-suffix.js",
    "prepare": "yarn build && husky install",
    "test": "jest",
    "test:e2e": "jest tests/e2e",
    "lint": "eslint src --ext .ts",
    "prettier": "prettier src",
    "majestic": "majestic"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "form-data": "^4.0.0",
    "oauth-1.0a": "^2.2.6"
  },
  "devDependencies": {
    "@qompium/eslint-config-q-node": "^1.2.9",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/humps": "^2.0.0",
    "@types/jest": "^26.0.20",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^8.1.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "jest-stare": "^2.2.2",
    "lint-staged": "^10.5.4",
    "nock": "^13.0.7",
    "npm-version-suffix": "galmail/npm-version-suffix",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.45.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3"
  },
  "jest-junit": {
    "outputDirectory": "test-results",
    "outputName": "jest-junit.xml"
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint --fix",
      "yarn prettier src --write"
    ]
  }
}
